fn calculate_pi(iterations: float) -> float:
    let pi = 0.0
    let sign = 1.0
    let k = 0.0
    
    while k < iterations:
        pi = pi + sign / (2.0 * k + 1.0)
        sign = -sign
        k = k + 1.0
    
    return pi * 4.0

fn main():
    let iterations = 100000000.0
    
    let _pi = calculate_pi(iterations)
