{
  "name": "OtterLang",
  "scopeName": "source.otterlang",
  "fileTypes": [
    "ot"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#builtins"
    },
    {
      "include": "#function-definition"
    },
    {
      "include": "#enum-definition"
    },
    {
      "include": "#enum-variant-definition"
    },
    {
      "include": "#struct-definition"
    },
    {
      "include": "#control-structures"
    },
    {
      "include": "#types"
    },
    {
      "include": "#enum-variants"
    },
    {
      "include": "#function-calls"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#identifiers"
    },
    {
      "include": "#module-imports"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.otterlang",
          "begin": "#",
          "end": "$",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.otterlang"
            }
          }
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.otterlang",
          "match": "\\b(if|elif|else|for|while|break|continue|return|match|case|pass|await|spawn)\\b"
        },
        {
          "name": "keyword.other.otterlang",
          "match": "\\b(fn|let|pub|struct|enum|type|trait|impl|use|from|as|in|and|or|not|is|lambda)\\b"
        },
        {
          "name": "constant.language.boolean.otterlang",
          "match": "\\b(true|false|None|none)\\b"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "support.type.primitive.otterlang",
          "match": "\\b(int|float|bool|string|str|unit|i32|i64|f64)\\b"
        },
        {
          "name": "meta.type.generic.otterlang",
          "match": "\\b([A-Z][a-zA-Z0-9_]*)\\s*(<)([^>]+)(>)",
          "captures": {
            "1": {
              "name": "support.type.otterlang"
            },
            "2": {
              "name": "punctuation.bracket.angle.otterlang"
            },
            "3": {
              "patterns": [
                {
                  "name": "support.type.generic.parameter.otterlang",
                  "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
                },
                {
                  "include": "#types"
                },
                {
                  "name": "punctuation.separator.otterlang",
                  "match": ","
                }
              ]
            },
            "4": {
              "name": "punctuation.bracket.angle.otterlang"
            }
          }
        },
        {
          "name": "support.type.result.otterlang",
          "match": "\\bResult\\b"
        },
        {
          "name": "support.type.option.otterlang",
          "match": "\\bOption\\b"
        },
        {
          "name": "support.type.otterlang",
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "builtins": {
      "patterns": [
        {
          "name": "support.function.builtin.otterlang",
          "match": "\\b(print|println|eprintln|len|cap|append|delete|enumerate|list_new|map_new|list_get|map_get|map_set|panic|recover|range|range_float|type_of|fields|str|stringify|try_func|try_result|try_error|defer_func|select|select_case)\\b"
        }
      ]
    },
    "function-definition": {
      "patterns": [
        {
          "name": "meta.function.otterlang",
          "begin": "\\b(pub\\s+)?fn\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.other.visibility.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "name": "entity.name.function.otterlang",
              "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
            },
            {
              "include": "#function-parameters"
            },
            {
              "include": "#type-annotation"
            }
          ]
        }
      ]
    },
    "control-structures": {
      "patterns": [
        {
          "name": "meta.control.if.otterlang",
          "begin": "\\b(if|elif)\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.control.else.otterlang",
          "begin": "\\belse\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.control.while.otterlang",
          "begin": "\\bwhile\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.control.for.otterlang",
          "begin": "\\bfor\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.control.match.otterlang",
          "begin": "\\bmatch\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.control.case.otterlang",
          "begin": "\\bcase\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "name": "variable.parameter.pattern.otterlang",
              "match": "\\b[a-z_][a-zA-Z0-9_]*\\b"
            },
            {
              "include": "#enum-variants"
            },
            {
              "include": "$self"
            }
          ]
        }
      ]
    },
    "function-parameters": {
      "patterns": [
        {
          "name": "variable.parameter.otterlang",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        },
        {
          "include": "#type-annotation"
        }
      ]
    },
    "type-annotation": {
      "patterns": [
        {
          "name": "punctuation.separator.type.otterlang",
          "match": ":"
        },
        {
          "name": "keyword.operator.arrow.otterlang",
          "match": "->"
        },
        {
          "include": "#types"
        }
      ]
    },
    "enum-definition": {
      "patterns": [
        {
          "name": "meta.enum.otterlang",
          "begin": "\\b(pub )?enum\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.other.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "name": "entity.name.type.enum.otterlang",
              "match": "\\b[A-Z][a-zA-Z0-9_]*(?:<[^>]+>)?\\b"
            },
            {
              "include": "#function-parameters"
            }
          ]
        }
      ]
    },
    "enum-variant-definition": {
      "patterns": [
        {
          "name": "meta.enum.variant.definition.otterlang",
          "match": "\\s*([A-Z][a-zA-Z0-9_]*)\\s*(:)\\s*(\\()([^)]*)(\\))",
          "captures": {
            "1": {
              "name": "entity.name.type.enum.variant.otterlang"
            },
            "2": {
              "name": "punctuation.definition.colon.otterlang"
            },
            "3": {
              "name": "punctuation.bracket.round.otterlang"
            },
            "4": {
              "patterns": [
                {
                  "include": "#types"
                },
                {
                  "name": "punctuation.separator.otterlang",
                  "match": ","
                }
              ]
            },
            "5": {
              "name": "punctuation.bracket.round.otterlang"
            }
          }
        }
      ]
    },
    "struct-definition": {
      "patterns": [
        {
          "name": "meta.struct.otterlang",
          "begin": "\\b(pub )?struct\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.other.otterlang"
            }
          },
          "end": ":",
          "patterns": [
            {
              "name": "entity.name.type.struct.otterlang",
              "match": "\\b[A-Z][a-zA-Z0-9_]*(?:<[^>]+>)?\\b"
            },
            {
              "include": "#function-parameters"
            }
          ]
        }
      ]
    },
    "enum-variants": {
      "patterns": [
        {
          "name": "entity.name.type.enum.variant.result.otterlang",
          "match": "\\bResult\\.(Ok|Err)\\b"
        },
        {
          "name": "entity.name.type.enum.variant.option.otterlang",
          "match": "\\bOption\\.(Some|None)\\b"
        },
        {
          "name": "entity.name.type.enum.variant.otterlang",
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\.[A-Z][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "function-calls": {
      "patterns": [
        {
          "name": "meta.function-call.builtin.otterlang",
          "begin": "\\b(print|println|eprint|eprintln|len|str|type|range|abs|min|max|round|floor|ceil|cap|append|delete|enumerate|stringify|type_of|fields)\\s*\\(",
          "beginCaptures": {
            "1": {
              "name": "support.function.builtin.otterlang"
            }
          },
          "end": "\\)",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.parameters.end.otterlang"
            }
          },
          "patterns": [
            {
              "include": "$self"
            }
          ]
        },
        {
          "name": "meta.function-call.otterlang",
          "begin": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\(",
          "beginCaptures": {
            "1": {
              "name": "entity.name.function.otterlang"
            }
          },
          "end": "\\)",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.parameters.end.otterlang"
            }
          },
          "patterns": [
            {
              "include": "$self"
            }
          ]
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.otterlang",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.otterlang",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.otterlang",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.otterlang",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.double.interpolated.otterlang",
          "begin": "f[\"']",
          "end": "[\"']",
          "patterns": [
            {
              "name": "constant.character.escape.otterlang",
              "match": "\\\\."
            },
            {
              "name": "meta.embedded.expression.otterlang",
              "begin": "\\{",
              "end": "\\}",
              "patterns": [
                {
                  "name": "variable.other.interpolation.otterlang",
                  "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
                },
                {
                  "include": "$self"
                }
              ]
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.integer.otterlang",
          "match": "\\b\\d[\\d_]*\\b"
        },
        {
          "name": "constant.numeric.float.otterlang",
          "match": "\\b\\d[\\d_]*\\.\\d[\\d_]*(?:[eE][+-]?\\d[\\d_]*)?\\b"
        },
        {
          "name": "constant.numeric.scientific.otterlang",
          "match": "\\b\\d[\\d_]*[eE][+-]?\\d[\\d_]*\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.comparison.otterlang",
          "match": "==|!=|<=|>=|<|>"
        },
        {
          "name": "keyword.operator.arithmetic.otterlang",
          "match": "[+\\-*/%]"
        },
        {
          "name": "keyword.operator.assignment.otterlang",
          "match": "=|\\+=|\\-=|\\*=|/=|%="
        },
        {
          "name": "keyword.operator.logical.otterlang",
          "match": "&&|\\|\\||!"
        },
        {
          "name": "keyword.operator.arrow.otterlang",
          "match": "->"
        },
        {
          "name": "keyword.operator.range.otterlang",
          "match": "\\.\\."
        },
        {
          "name": "keyword.operator.accessor.otterlang",
          "match": "\\."
        },
        {
          "name": "punctuation.separator.otterlang",
          "match": ","
        },
        {
          "name": "punctuation.bracket.square.otterlang",
          "match": "[\\[\\]]"
        },
        {
          "name": "punctuation.bracket.curly.otterlang",
          "match": "[{}]"
        },
        {
          "name": "punctuation.bracket.round.otterlang",
          "match": "[()]"
        },
        {
          "name": "punctuation.definition.colon.otterlang",
          "match": ":"
        }
      ]
    },
    "identifiers": {
      "patterns": [
        {
          "name": "variable.language.self.otterlang",
          "match": "\\bself\\b"
        },
        {
          "name": "variable.other.constant.otterlang",
          "match": "\\b[A-Z][A-Z0-9_]*\\b"
        },
        {
          "name": "variable.other.otterlang",
          "match": "\\b[a-z_][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "module-imports": {
      "patterns": [
        {
          "name": "keyword.other.import.otterlang",
          "match": "\\buse\\b"
        },
        {
          "name": "keyword.other.visibility.otterlang",
          "match": "\\bpub\\b"
        },
        {
          "name": "keyword.other.alias.otterlang",
          "match": "\\bas\\b"
        },
        {
          "name": "entity.name.namespace.otterlang",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*(?:/[a-zA-Z_][a-zA-Z0-9_]*)*\\b"
        }
      ]
    }
  }
}