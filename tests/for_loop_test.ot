fn test_for_loop():
    let sum = 0
    for i in 0..10:
        sum = sum + i
    # sum should be 45
    println(f"{sum}")

fn test_for_loop_nested():
    let total = 0
    for i in 0..5:
        for j in 0..3:
            total = total + 1
    # total should be 15
    println(f"{total}")

fn main():
    test_for_loop()
    test_for_loop_nested()
